CONFIG_CMDLINE="console=ttyS0,115200 ro rdinit=/usr/sbin/init"
CONFIG_CMDLINE_FORCE=y
CONFIG_BLK_DEV_INITRD=y
CONFIG_INITRAMFS_FORCE=y
